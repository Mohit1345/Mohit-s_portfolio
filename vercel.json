{
"builds": [
  {
    "src": "Portfolio/wsgi.py",
    "use": "@vercel/python"
  },
  {
    "src": "build_files.sh",
    "use": "@vercel/static-build",
    "config": {
        "distDir": "static"
    }
  }
],
"routes": [
    {
    "src": "/static/(.*)",
    "dest": "/static/$1"
    },
    {
        "src": "/(.*)",
        "dest": "Portfolio/wsgi.py"
    }
]
}
